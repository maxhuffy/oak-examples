{
	"name": "@luxonis/depthai-viewer-common",
	"version": "1.4.2",
	"description": "Common utilities and components for building a frontend application for DepthAI projects",
	"license": "ISC",
	"author": "Luxonis Holding Corporation",
	"type": "module",
	"main": "dist/index.js",
	"exports": {
		".": "./dist/index.js",
		"./styles": "./dist/index.css"
	},
	"scripts": {
		"build": "tsc --build && cp src/index.css ./dist/ && cp -r ./src/fonts ./dist/fonts",
		"build:clean": "rm -fr ../../node_modules/.tmp/tsconfig-common.tsbuildinfo dist && npm run build",
		"check-deps": "npm run deps:check -w @luxonis/depthai-web-viewer",
		"prepublishOnly": "npm run check-deps && npm run build:clean"
	},
	"publishConfig": {
		"access": "public"
	},
	"dependencies": {
		"@luxonis/visualizer-protobuf": "2.57.0",
		"@luxonis/depthai-pipeline-lib": "1.4.12",
		"@luxonis/common-fe-components": "1.23.3",
		"@luxonis/common-fe-utils": "1.23.3",
		"lodash": "^4.17.21",
		"zod": "^3.24.3",
		"react-resizable-panels": "^2.1.3"
	},
	"devDependencies": {
		"@luxonis-public/shadow-panda-preset": "^0.7.2"
	},
	"peerDependencies": {
		"@pandacss/dev": "^0.53.0",
		"react": "^18.3.1",
		"react-dom": "^18.3.1"
	}
}
