{
  "compilerOptions": {
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "isolatedModules": true,
    "esModuleInterop": true,
    "moduleResolution": "node16",
    "resolveJsonModule": true,
    "importsNotUsedAsValues": "remove",
    "target": "ESNext",
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "sourceMap": true,
    "composite": true,
    "incremental": true,
    "allowJs": true,
    "skipLibCheck": true,
    "downlevelIteration": true,
    "declaration": true,
    "declarationMap": true,
    "importHelpers": true,
    "maxNodeModuleJsDepth": 1,
    "jsx": "react-jsx",
    "module": "node16",
    "outDir": "./dist",
    "rootDir": "./src"
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx"
  ],
  "exclude": ["test/**/*.ts"],
  "references": []
}